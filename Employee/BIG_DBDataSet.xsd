                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT   ID, EMP_ID,  REPLACE(LTRIM(RTRIM(isnull(ad.NAME,''))),' ', ' ') + ' ' +
ISNULL(ad.DISTRICT,'') +' ' + 
isnull(ap.AMPHUR_NAME,'') +' ' + 
isnull(pv.PROVINCE_NAME,'') +' ' + 
isnull(ad.POSTCODE,'') as FULLADDRESS

FROM dbo.Address ad
LEFT JOIN PROVINCE pv on ad.PROVINCE_ID = pv.PROVINCE_ID
LEFT JOIN Amphur ap on ad.AMPHUR_ID = ap.AMPHUR_ID 

WHERE isnull(ADDRESSTYPE_ID,'') &lt;&gt; 1
and EMP_ID = @emp_id</CommandText>
