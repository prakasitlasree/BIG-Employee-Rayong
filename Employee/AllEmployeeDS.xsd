YEAR(e.DATEOFBIRTH) + 543 as YEAR,

CASE MONTH(e.DATEOFBIRTH)
WHEN 1 THEN 'มกราคม'
WHEN 2 THEN 'กุมภาพันธ์' 
WHEN 3 THEN 'มีนาคม' 
WHEN 4 THEN 'เมษายน' 
WHEN 5 THEN 'พฤษภาคม'
WHEN 6 THEN 'มิถุนายน'
WHEN 7 THEN 'กรกฎาคม'
